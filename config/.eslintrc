{
    "env": {
        "browser": true,
        "es2020": true
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
      "array-bracket-spacing": [1, "always"],
      "array-callback-return": 2,
      "arrow-body-style": [1, "as-needed"],
      "arrow-parens": [1, "as-needed"],
      "arrow-spacing": 1,
      "block-scoped-var": 1,
      "brace-style": 1,
      "class-methods-use-this": 1,
      "comma-dangle": [1, "never"],
      "comma-spacing": [1, {
        "before": false,
        "after": true
      }],
      "consistent-return": 1,
      "curly": [1, "multi"],
      "default-case": 1,
      "default-case-last": 1,
      "dot-location": [2, "property"],
      "dot-notation": 1,
      "eqeqeq": 1,
      "func-names": [2, "always"],
      "func-call-spacing": [1, "never"],
      "guard-for-in": 1,
      "indent": [2, 2],
      "key-spacing": [1, {
        "beforeColon": false,
        "afterColon": true
      }],
      "keyword-spacing": [1, {
        "before": true,
        "after": true
      }],
      "max-len": [2, {
        "code": 100,
        "ignoreUrls": true
      }],
      "no-alert": 2,
      "no-console": 1,
      "no-empty-function": 2,
      "no-fallthrough": 1,
      "no-floating-decimal": 2,
      "no-global-assign": 2,
      "no-implicit-coercion": 1,
      "no-implicit-globals": 1,
      "no-invalid-this": 2,
      "no-lonely-if": 0,
      "no-loop-func": 1,
      "no-magic-numbers": [1, {
        "ignore": [0, 1],
        "ignoreArrayIndexes": true
      }],
      "no-mixed-operators": 1,
      "no-multi-spaces": 1,
      "no-multi-str": 1,
      "no-nested-ternary": 0,
      "no-new": 2,
      "no-param-reassign": 1,
      "no-plusplus": 0,
      "no-proto": 2,
      "no-redeclare": 2,
      "no-trailing-spaces": 2,
      "no-unneeded-ternary": 1,
      "no-unused-vars": 1,
      "no-var": 2,
      "no-whitespace-before-property": 1,
      "object-curly-newline": [1, { "multiline": true }],
      "object-curly-spacing": [1, "always"],
      "object-shorthand": 1,
      "prefer-spread": 1,
      "radix": [1, "as-needed"],
      "semi": [2, "always"],
      "semi-style": [1, "last"],
      "space-before-function-paren": 1,
      "vars-on-top": 1,
      "yoda": 2
    }
}
